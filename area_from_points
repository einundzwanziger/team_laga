import json
import numpy as np
import pandas as pd

import matplotlib.pyplot as plt

df = pd.read_csv("/Users/leartsej/Desktop/repository/data/tempo-30-zonen.csv", sep=";")
breakpoint()
js = json.loads(df['Geo Shape'][0])
data = np.array(js['coordinates'][0])
fig, ax = plt.subplots()
ax.plot(data[:, 0], data[:, 1], ls="-", marker="o")
plt.show()

